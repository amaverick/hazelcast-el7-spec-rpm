HZ_CONFIG=-Dhazelcast.config=/etc/hazelcast/hazelcast.xml
HZ_HEAP_OPTS=-Xms2048m -Xmx2048m
HZ_JAVA_OPTS=-Djava.net.preferIPv4Stack=true
HZ_JMX_OPTS=-Dhazelcast.jmx=true -Dcom.sun.management.jmxremote -Dcom.sun.management.jmxremote.authenticate=true -Dcom.sun.management.jmxremote.password.file=/etc/hazelcast/jmxremote.password -Dcom.sun.management.jmxremote.access.file=/etc/hazelcast/jmxremote.access -Dcom.sun.management.jmxremote.ssl=false -Dcom.sun.management.jmxremote.port=32011 -Dcom.sun.management.jmxremote.rmi.port=32012
